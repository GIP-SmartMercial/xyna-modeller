{
    "name": "xyna",
    "title": "Xyna Modeller",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve --ssl=true --ssl-key=./ssl/server-public.pem",
        "build": "ng build xyna",
        "release": "ng build xyna --prod --extract-licenses",
        "release:de-DE": "ng build xyna --prod --extract-licenses --i18n-file ./projects/xyna/locale/messages.de-DE.xlf --i18n-format xlf --i18n-locale de-DE",
        "release:en-US": "ng build xyna --prod --extract-licenses --i18n-file ./projects/xyna/locale/messages.en-US.xlf --i18n-format xlf --i18n-locale en-US",
        "lint": "eslint --color -c .eslintrc.json --ext .ts ./projects/xyna/src/app",
        "lint:fix": "npm run lint -- --fix",
        "tsc": "tsc",
        "extract-i18n": "ng xi18n xyna --i18n-format xlf --output-path ./locale --i18n-locale en-US && ng run xyna:xliffmerge"
    },
    "dependencies": {
        "@angular/animations": "11.1.1",
        "@angular/cdk": "11.1.1",
        "@angular/common": "11.1.1",
        "@angular/compiler": "11.1.1",
        "@angular/core": "11.1.1",
        "@angular/forms": "11.1.1",
        "@angular/localize": "11.1.1",
        "@angular/material": "11.1.1",
        "@angular/platform-browser": "11.1.1",
        "@angular/platform-browser-dynamic": "11.1.1",
        "@angular/router": "11.1.1",
        "@angular/service-worker": "11.1.1",
        "@types/resize-observer-browser": "^0.1.7",
        "escape-string-regexp": "^5.0.0",
        "hammerjs": "2.0.8",
        "rxjs": "6.6.3",
        "three": "0.129.0",
        "tslib": "2.1.0",
        "zone.js": "0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1102.14",
        "@angular-eslint/eslint-plugin": "1.1.0",
        "@angular/cli": "11.1.2",
        "@angular/compiler-cli": "11.1.1",
        "@angular/language-service": "11.1.1",
        "@types/node": "14.14.22",
        "@types/three": "0.129.1",
        "@typescript-eslint/eslint-plugin": "4.15.0",
        "@typescript-eslint/parser": "4.15.0",
        "eslint": "7.19.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "31.6.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-zeta": "file:projects/xyna/src/app/zeta/lint/plugins/eslint-plugin-zeta",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    }
}
